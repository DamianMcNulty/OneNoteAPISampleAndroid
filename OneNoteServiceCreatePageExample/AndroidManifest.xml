<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onenoteservicecreatepageexample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />
    
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.onenoteservicecreatepageexample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.onenoteservicecreatepageexample.ResultsActivity"
            android:label="@string/title_activity_results"
            android:parentActivityName="com.example.onenoteservicecreatepageexample.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.onenoteservicecreatepageexample.MainActivity" />
        </activity>
    </application>

</manifest>
